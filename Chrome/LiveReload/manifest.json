{
  "name": "LiveReload",
  "version": "2.0.9",
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injected.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "icons": { "48": "icon48.png",
            "128": "icon128.png" },
  "browser_action": {
    "default_title": "Enable LiveReload",
    "default_icon": "IconDisabled.png"
  },
  "update_url": "http://download.livereload.com/LiveReload-Chrome-update.xml"
}
